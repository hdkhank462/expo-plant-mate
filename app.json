{
  "expo": {
    "name": "Plant Mate",
    "slug": "plant-mate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/icon.png",
    "scheme": "plantmate.hdkhanh",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/adaptive-icon/foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hdkhanh462.plantmate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Ứng dụng cần quyền truy cập vào ảnh của bạn."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
